[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[user]
    name = Collin Styles
    email = collingstyles@gmail.com
[alias]
    br = branch
    ci = commit
    co = checkout
    st = status
    unstage = reset HEAD --
    graph = log --graph --oneline --decorate --all
    graphme = log --graph --oneline --decorate
    last = log -1 HEAD
    discard = checkout --
    re = remote
    f = fetch
    fp = fetch --prune
    a = add
    ap = add --patch
    ai = add --interactive
    ca = commit --amend
    d = diff
    dc = diff --cached
    g = graph
    l = log
    ri = rebase --interactive
    ra = rebase --abort
    rc = rebase --continue
    sl = stash list
    sp = stash pop
    ss = stash show --patch
[log]
    date = local
[push]
    default = simple
[color]
    ui = auto
[fetch]
    prune = true
[core]
    quotepath = false
    editor = nvim
[rebase]
    autosquash = true
