[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[user]
    name = Collin Styles
    email = collingstyles@gmail.com
[alias]
    ci = commit
    st = status
    unstage = reset HEAD --
    graph = log --graph --oneline --decorate --all
    graphme = log --graph --oneline --decorate
    last = log -1 HEAD
    discard = checkout --
    f = fetch
    fa = fetch --all
    a = add
    ab = absorb
    abn = absorb --verbose --dry-run
    ap = add --patch
    ai = add --interactive
    br = branch
    brav = branch --all --verbose
    ca = commit --amend
    caa = commit --amend --no-edit
    cb = checkout -b
    cf = commit --fixup
    cl = clone
    co = checkout
    d = diff
    dc = diff --cached
    dhu = diff HEAD @{u}
    duh = diff @{u} HEAD
    g = graph
    l = log
    lo = log --oneline
    lp = log --patch
    lpr = log --patch --reverse
    m = merge
    ma = merge --abort
    p = push
    pl = pull
    r = reset
    ra = rebase --abort
    rb = rebase
    rc = rebase --continue
    re = restore
    rem = remote
    rh = reset --hard
    ri = rebase --interactive
    root = rev-parse --show-toplevel
    rs = rebase --skip
    rv = remote --verbose
    s = show
    sl = stash list
    ss = stash show --patch
    sw = switch
    swd = show --word-diff
    wd = diff --word-diff
    wt = worktree
[log]
    date = local
    follow = true
[push]
    default = simple
[color]
    ui = auto
[fetch]
    prune = true
[core]
    quotepath = false
    editor = nvim
[rebase]
    autosquash = true
[commit]
    verbose = true
[init]
    templateDir = ~/.git-templates
