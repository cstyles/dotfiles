[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[user]
    name = Collin Styles
    email = collingstyles@gmail.com
[alias]
    ci = commit
    st = status
    unstage = reset HEAD --
    graph = log --graph --oneline --decorate --all
    graphme = log --graph --oneline --decorate
    last = log -1 HEAD
    discard = checkout --
    f = fetch
    fp = fetch --prune
    a = add
    ap = add --patch
    ai = add --interactive
    br = branch
    brav = branch --all --verbose
    ca = commit --amend
    cb = checkout -b
    cf = commit --fixup
    co = checkout
    d = diff
    dc = diff --cached
    g = graph
    l = log
    m = merge
    ma = merge --abort
    p = push
    ra = rebase --abort
    rc = rebase --continue
    ri = rebase --interactive
    rs = rebase --skip
    re = remote
    rv = remote --verbose
    s = show
    sl = stash list
    ss = stash show --patch
    wd = diff --word-diff
[log]
    date = local
    follow = true
[push]
    default = simple
[color]
    ui = auto
[fetch]
    prune = true
[core]
    quotepath = false
    editor = nvim
[rebase]
    autosquash = true
[commit]
    verbose = true
[init]
    templateDir = ~/.git-templates
